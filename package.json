{
  "name": "ff-core-ui-demo",
  "version": "0.0.1",
  "description": "A demo for all the components in the Flimflam Core UI",
  "main": "index.js",
  "scripts": {
    "dev": "budo index.js --live --css build.css -- -t es2040",
    "build": "cp index.html dist/ && cp build.css dist/ && browserify index.js -t es2040 -o dist/index.js",
    "css": "postcss -w -c postcss-config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flimflamjs/core-ui-demo.git"
  },
  "keywords": [
    "flimflam"
  ],
  "author": "jay r bolton <jayrbolton@gmail.com> (http://jayrbolton.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flimflamjs/core-ui-demo/issues"
  },
  "homepage": "https://github.com/flimflamjs/core-ui-demo#readme",
  "dependencies": {
    "flimflam": "https://github.com/flimflamjs/flimflam.git",
    "ff-docs-theme": "https://github.com/flimflamjs/docs-theme.git"
  },
  "devDependencies": {
    "autoprefixer": "6.7.0",
    "browserify": "13.3.0",
    "budo": "9.4.5",
    "es2040": "1.2.4",
    "postcss-cli": "2.6.0",
    "postcss-import": "9.1.0"
  }
}
